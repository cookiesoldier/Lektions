<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:id="@+id/gggh">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Første App"
        android:id="@+id/Overskrift"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Tryk på knappen"
        android:id="@+id/textViewLeftTop"
        android:layout_below="@+id/editTextOne"
        android:layout_alignStart="@+id/buttonSmall" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Skift Navn"
        android:id="@+id/buttonOne"
        android:layout_below="@+id/textViewLeftTop"
        android:layout_alignStart="@+id/editTextOne" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Også mig"
        android:id="@+id/buttonSmall"
        android:layout_below="@+id/buttonOne"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="Name"
        android:ems="10"
        android:id="@+id/editTextOne"
        android:layout_below="@+id/Overskrift"
        android:layout_alignParentStart="true"
        android:layout_marginTop="54dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:src="@mipmap/ic_launcher"
        android:layout_below="@+id/Overskrift"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Vejledning"
        android:id="@+id/btnVejl"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/imageView" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Indstillinger"
        android:id="@+id/btnInstil"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

    <WebView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/webViewFront"
        android:layout_above="@+id/btnVejl"
        android:layout_marginBottom="66dp"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/btnInstil" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editTextBrowserLink"
        android:layout_above="@+id/webViewFront"
        android:layout_centerHorizontal="true"
        android:text="BrowserLink" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search"
        android:id="@+id/buttonSearch"
        android:layout_above="@+id/btnVejl"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
